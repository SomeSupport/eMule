					code += '<a href="?ses=' + session + '&w=transfer&op=getflc&file=' + filehash + '"><div class="menuitems"><img class="menuimages" src="l_getflc.gif"><img class="menuchecked" src="checked_no.gif">[GetFLC]&nbsp;</div></a>'
			}
		}
		
		if (cmts=="yes") {
			code += '<a href="?ses=' + session + '&w=commentlist&filehash=' + filehash+'" target="_blank"><div class="menuitems"><img class="menuimages" src="l_comments.gif"><img class="menuchecked" src="checked_no.gif">[FileComments]&nbsp;</div></a>'
