					code += '<a href="?ses=' + session + '&w=transfer&op=pause&file=' + filehash + '"><div class="menuitems"><img class="menuimages" src="l_pause.gif"><img class="menuchecked" src="checked_no.gif">[Pause]&nbsp;</div></a>'
				code += '<a href="?ses=' + session + '&w=transfer&op=cancel&file=' + filehash + '" onclick="return confirm(removeconfirm)"><div class="menuitems"><img class="menuimages" src="l_cancel.gif"><img class="menuchecked" src="checked_no.gif">[Cancel]&nbsp;</div></a>'
				code += '<a href="javascript:filerename(\'' + session + '\',\'' + filehash + '\',\'' + fname + '\')"><div class="menuitems"><img class="menuimages" src="l_rename.gif"><img class="menuchecked" src="checked_no.gif">[Rename]&nbsp;</div></a>'

        if(isgetflc == "enabled")
					code += '<a href="?ses=' + session + '&w=transfer&op=getflc&file=' + filehash + '"><div class="menuitems"><img class="menuimages" src="l_getflc.gif"><img class="menuchecked" src="checked.gif">[GetFLC]&nbsp;</div></a>'
