function downmenu(event,admin,finfo,ed2k,downstate,isgetflc,fname,session,filehash,commentinfo,downloadable)
{
	var code = new Array()
	ed2kinfo = ed2k
	iteminfo = finfo
	cmts = commentinfo
	removeconfirm = '[ConfirmCancel]' + fname

	if(downstate == "hashing" || downstate == "waitinghash")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif"><img class="menuchecked" src="checked_no.gif">[FileIsHashing]&nbsp;</div>'
	else if(downstate == "error")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif"><img class="menuchecked" src="checked_no.gif">[FileIsErroneous]&nbsp;</div>'
	else if(downstate == "completing")
		code = '<div class="menuitems"><img class="menuimages" src="l_hasherror.gif"><img class="menuchecked" src="checked_no.gif">[FileIsCompleting]&nbsp;</div>'
	else
	{
		code = '';

		if(admin == "admin")
		{
			if(downstate == "complete") {
				if (downloadable=="yes")
					code += '<a href="?ses=' + session + '&w=getfile&filehash=' + filehash +'"><div class="menuitems"><img class="menuimages" src="downloading.gif"><img class="menuchecked" src="checked_no.gif">[RunFile]&nbsp;</div></a>'
				code += '<a href="?ses=' + session + '&w=transfer&clearcompleted=' + filehash +'"><div class="menuitems"><img class="menuimages" src="l_clear.gif"><img class="menuchecked" src="checked_no.gif">[ClearCompleted]&nbsp;</div></a>'
			} else {
				if(downstate == "paused" || downstate == "stopped")
					code += '<a href="?ses=' + session + '&w=transfer&op=resume&file=' + filehash + '"><div class="menuitems"><img class="menuimages" src="l_resume.gif"><img class="menuchecked" src="checked_no.gif">[Resume]&nbsp;</div></a>'
				if(downstate != "stopped")
					code += '<a href="?ses=' + session + '&w=transfer&op=stop&file=' + filehash + '"><div class="menuitems"><img class="menuimages" src="l_stop.gif"><img class="menuchecked" src="checked_no.gif">[Stop]&nbsp;</div></a>'
